{
  "manifest_version": 2,
  "name": "Info Changer",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/content.js"],
      "css":["css/content.css"],
      "html":["index.html"]
    }
  ],
  "browser_action": {
    "default_icon": "image/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "activeTab",
    "desktopCapture"
  ],
  "web_accessible_resources": ["css/content.css","image/close.png","image/success.png"]
}
